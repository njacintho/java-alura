
public class TestaConexao {

	public static void main(String[] args) {
		
		Conexao conecta = null;
		try {
			conecta = new Conexao();
			conecta.leDados();
			conecta.fecha();
		}catch(IllegalStateException ex) {
			System.out.println("Houve erro na conexão");
		}finally {
			conecta.fecha();
		}

	}

}
